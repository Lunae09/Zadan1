#include <stdio.h>

int main()
{
   char str[100], napis;
   int i, frequency = 0;

   printf("Enter a string: ");
   gets(str);

   printf("Enter a character to find the frequency: ");
   scanf("%c",&napis);

   for(i = 0; str[i] != '\0'; ++i)
   {
       if(napis == str[i])
           ++frequency;
   }

   printf("Frequency of %c = %d", napis, frequency);

   return 0;
}
